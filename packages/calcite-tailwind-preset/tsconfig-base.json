{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["dom", "dom.iterable", "ES2023"],
    "module": "node18",
    "moduleResolution": "node16",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "target": "ES2022"
  },
  "exclude": ["node_modules", "dist"]
}
