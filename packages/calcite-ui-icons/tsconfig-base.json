{
  "compilerOptions": {
    "allowJs": true,
    "allowUnreachableCode": false,
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["dom", "dom.iterable", "ES2023"],
    "module": "esnext",
    "moduleResolution": "nodenext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "target": "ES2022"
  },
  "exclude": ["node_modules", "js"]
}
