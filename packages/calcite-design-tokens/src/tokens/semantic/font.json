{
  "semantic": {
    "font": {
      "family": {
        "default": {
          "attributes": {
            "category": "font",
            "group": "family"
          },
          "$type": "fontFamilies",
          "$value": "{core.font.family.avenirNext},{core.font.family.avenir},{core.font.family.helveticaNeue},{core.font.family.sansSerif}",
          "$description": "Primary font with fallbacks"
        },
        "code": {
          "attributes": {
            "category": "font",
            "group": "family"
          },
          "$type": "fontFamilies",
          "$value": "{core.font.family.monaco},{core.font.family.consolas},{core.font.family.andaleMono},{core.font.family.lucidaConsole},{core.font.family.monospace}",
          "$description": "Font family for code with fallbacks"
        }
      },
      "weight": {
        "light": {
          "attributes": {
            "category": "font",
            "group": "weight"
          },
          "$type": "fontWeights",
          "$value": "{core.font.weight.light}",
          "$description": "For Avenir Next World (secondary font family)"
        },
        "normal": {
          "extensions": {
            "calcite.deprecated": true
          },
          "attributes": {
            "category": "font",
            "group": "weight"
          },
          "$type": "fontWeights",
          "$value": "{core.font.weight.regular}",
          "$description": "For backwards compatibility only. This token will be removed from the published tokens in the next major in favor of the more descriptive word \"regular\""
        },
        "regular": {
          "attributes": {
            "category": "font",
            "group": "weight"
          },
          "$type": "fontWeights",
          "$value": "{core.font.weight.regular}"
        },
        "medium": {
          "attributes": {
            "category": "font",
            "group": "weight"
          },
          "$type": "fontWeights",
          "$value": "{core.font.weight.medium}"
        },
        "semibold": {
          "attributes": {
            "category": "font",
            "group": "weight"
          },
          "$type": "fontWeights",
          "$value": "{core.font.weight.demi}"
        },
        "bold": {
          "attributes": {
            "category": "font",
            "group": "weight"
          },
          "$type": "fontWeights",
          "$value": "{core.font.weight.demi}"
        }
      },
      "size": {
        "xs": {
          "attributes": {
            "category": "font",
            "group": "size"
          },
          "$type": "fontSizes",
          "$value": "{core.size.default.10}"
        },
        "sm": {
          "attributes": {
            "category": "font",
            "group": "size"
          },
          "$type": "fontSizes",
          "$value": "{core.size.default.12}"
        },
        "default": {
          "attributes": {
            "category": "font",
            "group": "size"
          },
          "$type": "fontSizes",
          "$value": "{core.size.default.14}"
        },
        "md": {
          "attributes": {
            "category": "font",
            "group": "size"
          },
          "$type": "fontSizes",
          "$value": "{core.size.default.16}"
        },
        "lg": {
          "attributes": {
            "category": "font",
            "group": "size"
          },
          "$type": "fontSizes",
          "$value": "{core.size.default.2} * 9"
        },
        "xl": {
          "attributes": {
            "category": "font",
            "group": "size"
          },
          "$type": "fontSizes",
          "$value": "{core.size.default.20}"
        },
        "xxl": {
          "attributes": {
            "category": "font",
            "group": "size"
          },
          "$type": "fontSizes",
          "$value": "{core.size.default.24}"
        }
      },
      "style": {
        "emphasis": {
          "attributes": {
            "category": "font",
            "group": "style"
          },
          "$type": "fontStyle",
          "$value": "{core.font.style.italic}",
          "$description": "used in ratings"
        }
      },
      "line-height": {
        "fixed": {
          "sm": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "fixed"
            },
            "$type": "lineHeights",
            "$value": "{core.size.default.12}"
          },
          "base": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "fixed"
            },
            "$type": "lineHeights",
            "$value": "{core.size.default.16}"
          },
          "lg": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "fixed"
            },
            "$type": "lineHeights",
            "$value": "{core.size.default.20}"
          },
          "xl": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "fixed"
            },
            "$type": "lineHeights",
            "$value": "{core.size.default.24}"
          }
        },
        "relative": {
          "default": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "relative"
            },
            "$type": "lineHeights",
            "$value": "{core.size.relative.auto}",
            "$description": "1"
          },
          "tight": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "relative"
            },
            "$type": "lineHeights",
            "$value": "{core.size.relative.125}",
            "$description": "1.25"
          },
          "snug": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "relative"
            },
            "$type": "lineHeights",
            "$value": "{core.size.relative.137}",
            "$description": "1.375"
          },
          "normal": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "relative"
            },
            "$type": "lineHeights",
            "$value": "{core.size.relative.150}",
            "$description": "1.5"
          },
          "relaxed": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "relative"
            },
            "$type": "lineHeights",
            "$value": "{core.size.relative.162}",
            "$description": "1.625"
          },
          "loose": {
            "attributes": {
              "category": "font",
              "group": "line-height",
              "kind": "relative"
            },
            "$type": "lineHeights",
            "$value": "{core.size.relative.200}",
            "$description": "2"
          }
        }
      },
      "letter-spacing": {
        "tight": {
          "attributes": {
            "category": "font",
            "group": "letter-spacing"
          },
          "$type": "letterSpacing",
          "$value": "{core.size.default.none} - .4 ",
          "$description": "Deprecated"
        },
        "normal": {
          "attributes": {
            "category": "font",
            "group": "letter-spacing"
          },
          "$type": "letterSpacing",
          "$value": "{core.size.default.none}",
          "$description": "Deprecated"
        },
        "wide": {
          "attributes": {
            "category": "font",
            "group": "letter-spacing"
          },
          "$type": "letterSpacing",
          "$value": "{core.size.default.none} + .4 ",
          "$description": "Deprecated"
        }
      },
      "paragraph-spacing": {
        "normal": {
          "attributes": {
            "category": "font",
            "group": "paragraph-spacing"
          },
          "$type": "paragraphSpacing",
          "$value": "{core.size.default.4}",
          "$description": "Deprecated"
        }
      },
      "text-decoration": {
        "none": {
          "attributes": {
            "category": "font",
            "group": "text-decoration"
          },
          "$type": "textDecoration",
          "$value": "{core.font.text-decoration.none}",
          "$description": "Deprecated"
        },
        "underline": {
          "attributes": {
            "category": "font",
            "group": "text-decoration"
          },
          "$type": "textDecoration",
          "$value": "{core.font.text-decoration.underline}",
          "$description": "Deprecated"
        }
      },
      "text-case": {
        "none": {
          "attributes": {
            "category": "font",
            "group": "text-case"
          },
          "$type": "textCase",
          "$value": "{core.font.text-case.none}",
          "$description": "Deprecated"
        },
        "uppercase": {
          "attributes": {
            "category": "font",
            "group": "text-case"
          },
          "$type": "textCase",
          "$value": "{core.font.text-case.uppercase}",
          "$description": "Deprecated"
        },
        "lowercase": {
          "attributes": {
            "category": "font",
            "group": "text-case"
          },
          "$type": "textCase",
          "$value": "{core.font.text-case.lowercase}",
          "$description": "Deprecated"
        },
        "capitalize": {
          "attributes": {
            "category": "font",
            "group": "text-case"
          },
          "$type": "textCase",
          "$value": "{core.font.text-case.capitalize}",
          "$description": "Deprecated"
        }
      }
    }
  }
}